generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserInfoRequest {
  id    Int     @id @default(autoincrement())
  email String @unique
}

model UserContact {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String  @unique
  phone     String?
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  Message   String?
}
